{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "codexing",
  "version": "0.1.0",
  "identifier": "com.david.codexing",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "codexing",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "resources/sqlite-vec/sqlite-vec-0.1.6-loadable-linux-aarch64.so",
      "resources/sqlite-vec/sqlite-vec-0.1.6-loadable-linux-x86_64.so",
      "resources/sqlite-vec/sqlite-vec-0.1.6-loadable-macos-aarch64.dylib",
      "resources/sqlite-vec/sqlite-vec-0.1.6-loadable-macos-x86_64.dylib",
      "resources/sqlite-vec/sqlite-vec-0.1.6-loadable-windows-x86_64.dll"
    ]
  }
}
